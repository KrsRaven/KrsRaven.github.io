          "_customHTML": "<section data-bs-version=\"5.1\" class=\"image1\" group=\"Image & Video\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverse\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Details\" name=\"showDetails\">\n        <input type=\"checkbox\" title=\"Line\" name=\"showLine\" condition=\"showDetails\" checked>\n        <input type=\"color\" title=\"Color\" name=\"lineColor\" value=\"#ffffff\" condition=\"showDetails && showLine\">\n        <input type=\"range\" title=\"Opacity\" name=\"lineOpacity\" min=\"0.1\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"showDetails && showLine\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\">\n        <select title=\"Align\" name=\"alignButtons\" condition=\"showButtons\">\n            <option value=\"justify-content-start\">Left</option>\n            <option value=\"justify-content-center\">Center</option>\n            <option value=\"justify-content-end\" selected>Right</option>\n        </select>\n        <header>Nutrition</header>\n        <input type=\"checkbox\" title=\"Show\" name=\"showNutrition\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" condition=\"showNutrition\">\n        <input type=\"checkbox\" title=\"Decoration\" name=\"decoration\" condition=\"showNutrition\" checked>\n        <input type=\"color\" title=\"Color\" name=\"decorationColor\" value=\"#000000\" condition=\"showNutrition && decoration\">\n        <header condition=\"showNutrition\">Card Background</header>\n        <input type=\"checkbox\" title=\"1 Transparent\" name=\"cardOneTr\" condition=\"showNutrition\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardOneBg\" value=\"#1f1437\" condition=\"showNutrition && !cardOneTr\">\n        <input type=\"checkbox\" title=\"2 Transparent\" name=\"cardTwoTr\" condition=\"showNutrition\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardTwoBg\" value=\"#1f1437\" condition=\"showNutrition && !cardTwoTr\">\n        <input type=\"checkbox\" title=\"3 Transparent\" name=\"cardThreeTr\" condition=\"showNutrition\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardThreeBg\" value=\"#1f1437\" condition=\"showNutrition && !cardThreeTr\">\n        <input type=\"checkbox\" title=\"4 Transparent\" name=\"cardFourTr\" condition=\"showNutrition\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardFourBg\" value=\"#1f1437\" condition=\"showNutrition && !cardFourTr\">\n        <header condition=\"showNutrition\">Card Border</header>\n        <input type=\"color\" title=\"One\" name=\"cardOneBorder\" value=\"#5b5b5b\" condition=\"showNutrition\">\n        <input type=\"color\" title=\"Two\" name=\"cardTwoBorder\" value=\"#5b5b5b\" condition=\"showNutrition\">\n        <input type=\"color\" title=\"Three\" name=\"cardThreeBorder\" value=\"#5b5b5b\" condition=\"showNutrition\">\n        <input type=\"color\" title=\"Four\" name=\"cardFourBorder\" value=\"#000000\" condition=\"showNutrition\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/snackm5/components/_images/